AACR,MAAA,EAAE,CAAC,GAAH,yBAAwB,KAAK,SAAL,CAAe,MAAf,CAAsB,QAAtB,EAAxB,GACG,GADH,CACO;AACH,QAAA,IAAI,YAAK,IAAI,IAAJ,GAAW,kBAAX,EAAL,cAAwC,IAAI,IAAJ,GAAW,kBAAX,EAAxC,CADD;AAEH,QAAA,GAAG,EAAE,YAAY,EAFd;AAGH,QAAA,OAAO,EAAE,aAAa;AAHnB,OADP,EAMG,IANH,CAMQ,YAAM;AACV,QAAA,OAAO,CAAC,GAAR,CAAY,YAAZ;AACD,OARH;AASD,KAZU;AAaX,IAAA,MAbW,oBAaF;AACP,MAAA,EAAE,CAAC,GAAH,yBAAwB,CAAC,KAAK,SAAL,CAAe,MAAf,GAAwB,CAAzB,EAA4B,QAA5B,EAAxB,GACG,MADH,GAEG,IAFH,CAEQ,YAAM;AACV,QAAA,OAAO,CAAC,GAAR,CAAY,cAAZ;AACD,OAJH;AAKD;AAnBU;AAbI,CAAf","sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <p>\n      Fo \n      <a href=\"https://cli.vuejs.org\" target=\"_blank\" rel=\"noopener\">vue-cli documentation</a>.\n    </p>\n       \n  </div>\n\n<div style=\"margin: 0 auto;width: 300px;\">\n  <div v-for=\"(data, index) in documents\" :key=\"index\" style=\"text-align:left;\">\n    {{ data.qno.toString().padStart(4, '0') }} |\n    {{ data.correct }} |\n    {{ data.date }}\n  </div>\n</div>\n\n<div> {{ this.getRandom }} </div>\n\n<div style=\"margin-top:20px\">\n  <button @click=\"add\" style=\"margin-right:20px\">Add</button>\n  <button @click=\"remove\">Remove</button>\n</div>\n \n</template>\n\n<script>\nimport HelloWorld from '@/components/HelloWorld.vue';\nimport Chance from 'chance';\nimport { db } from '../db';\n\nconst getRandomBool = () => {\n  const c = new Chance();\n  return c.bool();\n};\n\nconst getRandomInt = () => {\n  const c = new Chance();\n  return c.integer({ min: 1, max: 9999 });\n};\n\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      documents: [],\n    };\n  },\n  firebase: {\n    documents: db.ref('tests/commits'),\n  },\n  components: {\n    HelloWorld,\n  },\n  methods: {\n    // 獨立抽出\n    add() {\n  db.ref(`tests/commits/${this.documents.length.toString()}`)\n    .set({\n      date: `${new Date().toLocaleDateString()} ${new Date().toLocaleTimeString()}`,\n      qno: getRandomInt(),\n      correct: getRandomBool(),\n    })\n    .then(() => {\n      console.log('data added');\n    });\n},\nremove() {\n  db.ref(`tests/commits/${(this.documents.length - 1).toString()}`)\n    .remove()\n    .then(() => {\n      console.log('data removed');\n    });\n},\n  },\n};\n\n  \n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":""}]}