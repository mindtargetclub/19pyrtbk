vB;AAAA,kBAAA;AAAA;;;6DAnBH,mBAAA,CAOM,KAPN,EAAA,UAAA,EAOM,CANJ,mBAAA,CAAkB,IAAlB,EAAkB,IAAlB,EAAkB,gBAAA,CAAX,IAAA,CAAA,GAAW,CAAlB,EAAU;AAAA;AAAV,GAMI,EALJ,UAKI,CAPN,C,EASF,mBAAA,CAMM,KANN,EAAA,UAAA,EAMM,E,kBALJ,mBAAA,CAIM,SAJN,EAIM,IAJN,EAIM,WAAA,CAJuB,KAAA,CAAA,SAIvB,EAJgC,UAAzB,IAAyB,EAAnB,KAAmB,EAAd;yBAAxB,mBAAA,CAIM,KAJN,EAIM;AAJmC,MAAA,GAAG,EAAE,KAIxC;AAJ+C,MAAA,KAAwB,EAAxB;AAAA,sBAAA;AAAA;AAI/C,KAJN,E,iBACK,IAAI,CAAC,GAAL,CAAS,QAAT,GAAoB,QAApB,CAA4B,CAA5B,EAA4B,GAA5B,C,IAAuC,K,GAC1C,gBAAA,CAAG,IAAI,CAAC,OAAR,C,GAAkB,K,GAClB,gBAAA,CAAG,IAAI,CAAC,IAAR,CAHF,EAGc;AAAA;AAHd,K;GAIM,CAJN,E;;AAAA,GAKI,EANN,C,EAQA,mBAAA,CAAiC,KAAjC,EAAiC,IAAjC,EAAiC,gBAAA,CAAA,KAAnB,SAAmB,CAAjC,EAAuB;AAAA;AAAvB,G,EAEA,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,mBAAA,CAA2D,QAA3D,EAA2D;AAAlD,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,GAAA,IAAA,QAAA,CAAA,GAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAA6C;AAAtC,IAAA,KAAyB,EAAzB;AAAA,sBAAA;AAAA;AAAsC,GAA3D,EAA+C,KAA/C,CAEI,EADJ,mBAAA,CAAuC,QAAvC,EAAuC;AAA9B,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,MAAA,IAAA,QAAA,CAAA,MAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAyB,GAAvC,EAAwB,QAAxB,CACI,CAHN,C","sourcesContent":["<template>\n  <div class=\"hello\">\n    <h1>{{ msg }}</h1>\n    <p>\n      Fo \n      <a href=\"https://cli.vuejs.org\" target=\"_blank\" rel=\"noopener\">vue-cli documentation</a>.\n    </p>\n       \n  </div>\n\n<div style=\"margin: 0 auto;width: 300px;\">\n  <div v-for=\"(data, index) in documents\" :key=\"index\" style=\"text-align:left;\">\n    {{ data.qno.toString().padStart(4, '0') }} |\n    {{ data.correct }} |\n    {{ data.date }}\n  </div>\n</div>\n\n<div> {{ this.getRandom }} </div>\n\n<div style=\"margin-top:20px\">\n  <button @click=\"add\" style=\"margin-right:20px\">Add</button>\n  <button @click=\"remove\">Remove</button>\n</div>\n \n</template>\n\n<script>\nimport HelloWorld from '@/components/HelloWorld.vue';\nimport Chance from 'chance';\nimport { db } from '../db';\n\nconst getRandomBool = () => {\n  const c = new Chance();\n  return c.bool();\n};\n\nconst getRandomInt = () => {\n  const c = new Chance();\n  return c.integer({ min: 1, max: 9999 });\n};\n\nexport default {\n  name: 'Home',\n  data() {\n    return {\n      documents: [],\n    };\n  },\n  firebase: {\n    documents: db.ref('tests/commits'),\n  },\n  components: {\n    HelloWorld,\n  },\n  methods: {\n    // 獨立抽出\n    add() {\n  db.ref(`tests/commits/${this.documents.length.toString()}`)\n    .set({\n      date: `${new Date().toLocaleDateString()} ${new Date().toLocaleTimeString()}`,\n      qno: getRandomInt(),\n      correct: getRandomBool(),\n    })\n    .then(() => {\n      console.log('data added');\n    });\n},\nremove() {\n  db.ref(`tests/commits/${(this.documents.length - 1).toString()}`)\n    .remove()\n    .then(() => {\n      console.log('data removed');\n    });\n},\n  },\n};\n\n  \n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"sourceRoot":""}]}